{
  "name": "domainCode",
  "type": "module",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "node -r ./scripts/node-preload.cjs ./node_modules/nuxi/bin/nuxi.mjs build",
    "dev": "node -r ./scripts/node-preload.cjs ./node_modules/nuxi/bin/nuxi.mjs dev --host 127.0.0.1 --port 3000",
    "dev:lan": "node -r ./scripts/node-preload.cjs ./node_modules/nuxi/bin/nuxi.mjs dev --host 0.0.0.0 --port 3000",
    "generate": "node -r ./scripts/node-preload.cjs ./node_modules/nuxi/bin/nuxi.mjs generate",
    "preview": "node -r ./scripts/node-preload.cjs ./node_modules/nuxi/bin/nuxi.mjs preview",
    "start": "node .output/server/index.mjs",
    "tenant:add": "node ./scripts/tenant-manager.mjs add",
    "tenant:append": "node ./scripts/tenant-manager.mjs append",
    "tenant:disable": "node ./scripts/tenant-manager.mjs disable",
    "tenant:rotate-key": "node ./scripts/tenant-manager.mjs rotate-key"
  },
  "dependencies": {
    "imapflow": "^1.2.8",
    "mailparser": "^3.9.3",
    "nuxt": "3.10.3",
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "6.10.4",
    "@types/mailparser": "^3.4.6",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "overrides": {
    "pino": "^9.0.0"
  }
}
